{
  "name": "vnmd",
  "displayName": "vnmd",
  "description": "highlighting vnmd scripts",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "vnmd",
      "aliases": ["vnmd", "vnmd"],
      "extensions": [".scr"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "vnmd",
      "scopeName": "source.vnmd",
      "path": "./syntaxes/vnmd.tmLanguage.json"
    }]
  }
}
